$("#message-errors").html('<br>' +
  '<% if flash.notice %>' +
    '<div class="alert alert-danger">' +
    '<%=flash.notice.each do |message| %>' +
      '<span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span>' +
      '<%= message %> ' +
    '<% end %>' +
  '</div>' +
'<%end%>');
