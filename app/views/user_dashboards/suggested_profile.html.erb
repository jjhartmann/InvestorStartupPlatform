<!-- <br><br><br><br><br><br><br><br><%#= @suggested_user.name %> -->
<%= content_for :body_class, "dashbord" %>
<%= content_for :profile_active_class, "active" %>

<div class="main-body">
  <div class="container side-deatail" style="transition: none; width: 100%;">
    <div class="col-xs-12">
      <div class="profile-tabs">
        <div id="home" class="editable-contact-info edit-mode-disabled">
          <div class="tab-title">Profile</div>
          <div class="main-start">
            <div class="box-tab">
              <div class="form-group">
                 <div class="circle">
                    <%= image_tag @suggested_user.photo_url, class: "display-mode-value"%>
                 </div>
               </div>
                <div class="form-group">
                  <h2>
                    <i aria-hidden="true" class="fa fa-user"></i>
                    <span class="display-mode-value"><%= @suggested_user.name %></span>
                  </h2>
                </div>
                <div class="form-group">
                  <p><i aria-hidden="true" class="fa fa-bank"></i><span class="display-mode-value">Loerm ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been the industry's standard dummy text ever since the 1500s, when an unknown printer took a galley of type and scrambled it to make a type specimen book.</span></p>
                </div>

              <% if @suggested_user.profilable_type == "UserProfile" %>
                  <div class="form-group">
                    <div class="user-positon"><i aria-hidden="true" class="fa fa-industry"></i><span class="display-mode-value"><%= @suggested_user.profilable.industry %></span></div>
                  </div>
                  <div class="form-group">
                    <div class="user-positon"><i aria-hidden="true" class="fa fa-briefcase"></i><span class="display-mode-value"><%= @suggested_user.profilable.profession %></span></div>
                  </div>
                  <div class="form-group">
                    <div class="user-positon"><i aria-hidden="true" class="fa fa-graduation-cap"></i><span class="display-mode-value"><%= @suggested_user.profilable.skills %></span></div>
                  </div>
                  <div class="form-group">
                    <div class="user-positon"><i aria-hidden="true" class="fa fa-lightbulb-o"></i><span class="display-mode-value"><%= @suggested_user.profilable.something_cool %></span></div>
                  </div>
              <% else %>
                <div class="form-group">
                  <div class="user-positon"><i aria-hidden="true" class="fa fa-tag"></i><span class="display-mode-value"><%= @suggested_user.profilable.tagline %></span></div>
                </div>
                <div class="form-group">
                  <div class="user-positon"><i aria-hidden="true" class="fa fa-money"></i><span class="display-mode-value"><%= @suggested_user.profilable.funds_to_offer %></span></div>
                </div>
                <div class="form-group">
                  <div class="user-positon"><i aria-hidden="true" class="fa fa-file"></i><span class="display-mode-value"><%= @suggested_user.profilable.description %></span></div>
                </div>
              <% end %>
              <div class="form-group">
                <div class="user-positon">
                  <i aria-hidden="true" class="fa fa-envelope"></i>
                  <span class="display-mode-value"><%= @suggested_user.email %></span>
                </div>
              </div>
              <div class="event-btn">
                <button type="button" class="btn btn-default js-cancel">Cancel</button>
              </div>
              <div class="buttons">
                <%= link_to "#" do %>
                  <div class="edit"><i class="fa fa-comments" aria-hidden="true"></i></div>
                <% end %>
                <a href="#"><div class="edit share"><i class="fa fa-share" aria-hidden="true"></i></div></a>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
</div>
