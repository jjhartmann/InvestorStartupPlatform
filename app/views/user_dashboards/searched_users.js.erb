$("#all_users .user-follow").html('<ul class="clearfix">'+
                                    '<% if @all_users.count > 0 %>'+
                                    '<% @all_users.each do |user| %>'+
                                      '<li id="user_<%= user.id %>" class="clickable-row" data-href=\'/user_profiles/<%= current_user.id%>?suggested_profile=<%= user.id %>\'>'+
                                        '<div class="tag-1">'+
                                          '<div class="follow-pro">'+
                                            '<div class="pic-user"><%= image_tag user.photo_avatar %></div>'+
                                          '</div>'+
                                        '</div>'+
                                        '<div class="tag-2">'+
                                          '<h2><%= user.name %></h2>'+
                                          '<div class="tag-line-pro">Developer, Gujarat Housing Board</div>'+
                                        '</div>'+
                                        '<div class="tag-3">'+
                                          '<div class="action-tag">'+
                                            '<ul class="menu cf">'+
                                              '<li>'+
                                                '<%= link_to @user.is_following?(user) ? "Following" : "Follow", "#", class: "follow", "data-id": "#{user.id}", "data-type": "#{user.profilable_type}", remote: true %>'+
                                              '</li>'+
                                            '</ul>'+
                                          '</div>'+
                                        '</div>'+
                                      '</li>'+
                                    '<% end %>'+
                                    '<% else %>'+
                                      '<div class="notes-alert">'+
                                          '<div class="alert alert-warning">'+
                                            '<strong>No User</strong> found.'+
                                          '</div>'+
                                      '</div>'+
                                    '<% end %>'+
                                  '</ul>');


