$(".network").html("<%= @network_count %> <%= 'Person'.pluralize(@network_count)%>");
$(".connection").html("<%= @connection_count %>");
$("#connections_list").html('<%if @connected_users.count > 0 %>'+
                              '<% @connected_users.each do |connection| %>' +
                                '<li>'+
                                  '<% if connection.profilable_type == "UserProfile" %>'+
                                    '<%=j render "global_partials/user_profile_plug", user: connection %>'+
                                  '<% else %>'+
                                    '<%=j render "global_partials/investor_profile_plug", investor: connection %>'+
                                  '<% end %>'+
                                '</li>'+
                                '<% end %>'+
                              '<% else %>'+
                                '<div>You have no Connections</div>'+
                              '<% end %>');
