<%= simple_form_for(@news_feed) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :external_link %>
    <%= f.input :photo, as: :file %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
