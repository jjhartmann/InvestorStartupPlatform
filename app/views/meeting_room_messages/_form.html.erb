<%= simple_form_for(@meeting_room_message) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :meeting_room %>
    <%= f.association :meeting_member %>
    <%= f.association :message %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
